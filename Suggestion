import random

print("____________SUGGESTION____________")
print("")

choices = []

num = int(input("Enter the no. of things you wanna do : "))

for i in range(num):
  user_inputs = input("\nEnter thing {} : ".format(i+1))
  choices.append(user_inputs)
print("")
print(choices)
print("")
x = choices[random.randrange(0,num)]

print(x)





